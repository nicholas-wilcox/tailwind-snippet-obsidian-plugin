{
	"name": "unofficial-tailwindcss-obsidian-plugin",
	"version": "0.4.0",
	"description": "Use TailwindCSS utility classes in your Obsidian vault's markup.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"Obsidian",
		"TailwindCSS",
		"PostCSS"
	],
	"author": "Nicholas Wilcox <nwilcox@linicks.dev>",
	"repository": {
		"type": "git",
		"url": "https://github.com/nicholas-wilcox/unofficial-tailwindcss-obsidian-plugin.git"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^22.0.0",
		"@types/postcss-prefix-selector": "^1.15.0",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"builtin-modules": "^4.0.0",
		"esbuild": "^0.23.0",
		"obsidian": "latest",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"autoprefixer": "^10.4.14",
		"postcss": "^8.4.24",
		"postcss-prefix-selector": "^1.16.0",
		"tailwindcss": "^3.3.2"
	}
}
